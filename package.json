{
  "name": "mintworks",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^3.6.2",
    "@liveblocks/client": "^0.16.3",
    "@liveblocks/react": "^0.16.3",
    "@liveblocks/redux": "^0.16.2",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^28.1.8",
    "jest": "^28.1.3",
    "nightmare": "^3.0.2",
    "nightmare-react-utils": "^1.0.4",
    "node-fetch": "^3.2.10",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-countdown-circle-timer": "^3.0.9",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.2",
    "ts-jest": "^28.0.8"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "build-css": "node-sass --include-path ./node_modules src/assets/scss -o ./src/assets/css --recursive",
    "watch-css": "npm run build-css && node-sass --include-path ./node_modules src/assets/scss -o src/assets/scss --watch --recursive",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/nightmare": "^2.10.6",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-redux": "^5.0.21",
    "@types/webpack": "^5.28.0",
    "bootstrap-scss": "^4.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-redux": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^28.1.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "react-scripts": "^5.0.1"
  }
}
